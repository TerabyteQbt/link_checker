#!/bin/bash

eval export JAVA_HOME=\$JAVA_${QBT_ENV_JDK}_HOME

set -e

$INPUT_ARTIFACTS_DIR/weak/qbt_fringe.wrapper_generator.release/strong/qbt_fringe.wrapper_generator.release/bin/wrapper_generator $INPUT_ARTIFACTS_DIR/weak/qbt_fringe.link_checker.main/strong $OUTPUT_ARTIFACTS_DIR \
    link_checker qbt.fringe.link_checker.Main \

cp LICENSE "$OUTPUT_ARTIFACTS_DIR"

